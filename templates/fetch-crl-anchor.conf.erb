# <%= @anchorname -%>.conf file installed with puppet.

[<%= @anchorname -%>]
<% if @agingtollerance -%>
agingtolerance = <%= @agingtollerance %>
<% end -%>
<% if @nowarnings -%>
nowarnings
<% end -%>
<% if @noerrors -%>
noerrors
<% end -%>
<% if @httptimeout -%>
httptimeout = <%= @httptimeout %>
<% end -%>
<% if @logmode -%>
logmode = <%= @logmode %>
<% end -%>
<% if @crl_url and @crl_url.length >= 1 -%>
crl_url.1 = <%= @crl_url.join(';') %>
<% end -%>


# end of <%= @anchorname -%>.conf  managed by puppet.
